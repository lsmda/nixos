input {
    keyboard {
        xkb {
            layout "pt"
        }
        repeat-delay 400
        repeat-rate 40
    }
    touchpad {
        tap
        dwt
        accel-speed -0.1
        natural-scroll
        accel-profile "flat"
        scroll-method "two-finger"
    }
    disable-power-key-handling
    warp-mouse-to-focus
}
output "eDP-1" {
    mode "1920x1280@60.001"
    scale 1.5
}
